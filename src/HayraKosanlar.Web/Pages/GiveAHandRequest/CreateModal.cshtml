@page
@using HayraKosanlar.Localization
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model HayraKosanlar.Web.Pages.GiveAHandRequest.CreateModalModel
@inject IStringLocalizer<HayraKosanlarResource> L
@{
    Layout = null;
}
<abp-dynamic-form abp-model="GiveAHandRequest" asp-page="/GiveAHandRequest/CreateModal">
    <abp-modal>
        <abp-modal-header title="@L["NewGiveAHandRequest"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>