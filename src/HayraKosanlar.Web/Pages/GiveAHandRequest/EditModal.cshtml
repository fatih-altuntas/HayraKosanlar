@page
@using HayraKosanlar.Localization
@using HayraKosanlar.Web.Pages.GiveAHandRequest
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@inject IStringLocalizer<HayraKosanlarResource> L
@model HayraKosanlar.Web.Pages.GiveAHandRequest.EditModalModel
@{
    Layout = null;
}
<abp-dynamic-form abp-model="GiveAHandRequest" asp-page="/GiveAHandRequest/EditModal">
    <abp-modal>
        <abp-modal-header title="@L["UpdateGiveAHandRequest"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-input asp-for="Id" />
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>